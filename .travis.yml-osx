language: c

os: osx

osx_image: xcode7.3

install:
  - brew update
  - brew install python
  - brew install python3
  - brew install docker
  - brew install docker-machine
  - pip3 install virtualenv
  - make virtualenv

script:
  - docker version
  - make test-all

notifications:
  irc:
    - irc.freenode.org#tsadm
